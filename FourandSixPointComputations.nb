Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{"d", ",", "x", ",", "k", ",", "j"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"thetaest", "[", 
    RowBox[{"x_", ",", "d_", ",", "j_"}], "]"}], ":=", 
   RowBox[{"1", "+", 
    RowBox[{"2", 
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"-", "d"}], " ", 
          RowBox[{"k", "^", "2"}]}], "]"}], 
        RowBox[{"Cos", "[", 
         RowBox[{"2", "Pi", " ", "k", " ", "x"}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", "j"}], "}"}]}], "]"}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"thetaestt", "[", 
    RowBox[{"t_", ",", "d_", ",", "j_"}], "]"}], ":=", 
   RowBox[{"1", "+", 
    RowBox[{"2", 
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"-", "d"}], " ", 
          RowBox[{"k", "^", "2"}]}], "]"}], 
        RowBox[{"Cos", "[", 
         RowBox[{"k", " ", 
          RowBox[{"ArcCos", "[", "t", "]"}]}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", "j"}], "}"}]}], "]"}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"thetaestprime", "[", 
    RowBox[{"x_", ",", "d_", ",", "j_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"-", "2"}], 
    RowBox[{"Sum", "[", 
     RowBox[{
      RowBox[{"2", "Pi", " ", "k", " ", 
       RowBox[{"Exp", "[", 
        RowBox[{
         RowBox[{"-", "d"}], " ", 
         RowBox[{"k", "^", "2"}]}], "]"}], 
       RowBox[{"Sin", "[", 
        RowBox[{"2", "Pi", " ", "k", " ", "x"}], "]"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"k", ",", "j"}], "}"}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"thetaesttprime", "[", 
    RowBox[{"t_", ",", "d_", ",", "j_"}], "]"}], ":=", 
   RowBox[{"2", 
    RowBox[{"Sum", "[", " ", 
     RowBox[{
      RowBox[{"k", " ", 
       RowBox[{"Exp", "[", 
        RowBox[{
         RowBox[{"-", "d"}], " ", 
         RowBox[{"k", "^", "2"}]}], "]"}], 
       RowBox[{
        RowBox[{"Sin", "[", 
         RowBox[{"k", " ", 
          RowBox[{"ArcCos", "[", "t", "]"}]}], "]"}], "/", 
        RowBox[{"Sqrt", "[", 
         RowBox[{"1", "-", 
          RowBox[{"t", "^", "2"}]}], "]"}]}]}], ",", 
      RowBox[{"{", 
       RowBox[{"k", ",", "j"}], "}"}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"thetaesttprime", "[", 
    RowBox[{"1", ",", "d_", ",", "j_"}], "]"}], ":=", 
   RowBox[{"2", 
    RowBox[{"Sum", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"k", "^", "2"}], 
       RowBox[{"Exp", "[", 
        RowBox[{
         RowBox[{"-", "d"}], " ", 
         RowBox[{"k", "^", "2"}]}], "]"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"k", ",", "j"}], "}"}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"thetaesttprime", "[", 
    RowBox[{
     RowBox[{"-", "1"}], ",", "d_", ",", "j_"}], "]"}], ":=", 
   RowBox[{"2", 
    RowBox[{"Sum", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"k", "^", "2"}], 
       RowBox[{"Exp", "[", 
        RowBox[{
         RowBox[{"-", "d"}], " ", 
         RowBox[{"k", "^", "2"}]}], "]"}], 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"-", "1"}], ")"}], "^", 
        RowBox[{"(", 
         RowBox[{"k", "+", "1"}], ")"}]}]}], ",", 
      RowBox[{"{", 
       RowBox[{"k", ",", "j"}], "}"}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"thetaestprime2", "[", 
   RowBox[{"x_", ",", "d_", ",", "j_"}], "]"}], ":=", 
  RowBox[{"Sum", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"Exp", "[", 
      RowBox[{
