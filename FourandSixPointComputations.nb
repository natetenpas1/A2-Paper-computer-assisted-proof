   RowBox[{"maps_", ",", "a_", ",", "b_", ",", "n_", ",", "rad_", ",", "u_"}],
    "]"}], ":=", 
  RowBox[{"Apply", "[", 
   RowBox[{"And", ",", "\[IndentingNewLine]", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"minmakerx", "[", 
        RowBox[{"maps", ",", 
         RowBox[{"a", "+", 
          RowBox[{"k", "*", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"b", "-", "a"}], ")"}], "/", "n"}]}]}], ",", 
         RowBox[{"a", "+", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"k", "+", "1"}], ")"}], "*", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"b", "-", "a"}], ")"}], "/", "n"}]}]}], ",", "rad", ",", 
         "u"}], "]"}], ">", "0"}], ",", 
      RowBox[{"{", 
       RowBox[{"k", ",", "0", ",", 
        RowBox[{"n", "-", "1"}]}], "}"}]}], "]"}]}], "]"}]}]], "Input",
 CellLabel->
  "In[3319]:=",ExpressionUUID->"6c1c205e-8e79-4bdc-85ce-a9a64db6008a"],

