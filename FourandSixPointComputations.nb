Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Val6", "[", "u_", "]"}], "=", 
   RowBox[{
    RowBox[{"12", "u", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "-", "e"}], ")"}], "/", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "+", "e"}], ")"}], 
        RowBox[{"Sin", "[", 
         RowBox[{"2", "Pi", " ", "u"}], "]"}]}], ")"}]}]}], "//", 
    "Simplify"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Val7", "[", "u_", "]"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"2", "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "+", 
         RowBox[{"6", "e"}]}], ")"}], "/", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"Exp", "[", 
          RowBox[{"a", "/", "3"}], "]"}], 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"a10", "[", 
            RowBox[{"-", "1"}], "]"}], "+", 
           RowBox[{"t2", " ", 
            RowBox[{"a02", "[", "1", "]"}]}]}], ")"}]}], ")"}]}]}], "/.", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"a", "->", 
        RowBox[{"96", "/", "10"}]}], ",", " ", 
       RowBox[{"t2", "->", 
        RowBox[{"Cos", "[", 
         RowBox[{"2", "Pi", " ", "u"}], "]"}]}]}], "}"}]}], "//", 
    "Simplify"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"Plot", "[", 
  RowBox[{
