Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{"f1", ",", "f2"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{"For", " ", "[", 
  RowBox[{
   RowBox[{"i", "=", "1"}], ",", 
   RowBox[{"i", "<=", 
    RowBox[{"Length", "[", "lowhigh", "]"}]}], ",", 
   RowBox[{"i", "++"}], ",", 
   RowBox[{"For", "[", 
    RowBox[{
     RowBox[{"j", "=", "1"}], ",", 
     RowBox[{"j", "<=", " ", 
      RowBox[{"Length", "[", "boundarypoints", "]"}]}], ",", 
     RowBox[{"j", "++"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"f1", "[", 
       RowBox[{"0", ",", 
        RowBox[{"boundarypoints", "[", 
         RowBox[{"[", "j", "]"}], "]"}], ",", 
        RowBox[{"lowhigh", "[", 
         RowBox[{"[", "i", "]"}], "]"}]}], "]"}], "=", 
      RowBox[{
       RowBox[{"thetaestt", "[", 
