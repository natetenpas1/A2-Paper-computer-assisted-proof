    RowBox[{"-", "2"}], 
    RowBox[{"Sum", "[", 
     RowBox[{
      RowBox[{"2", "Pi", " ", "k", " ", 
       RowBox[{"Exp", "[", 
        RowBox[{
         RowBox[{"-", "d"}], " ", 
         RowBox[{"k", "^", "2"}]}], "]"}], 
       RowBox[{"Sin", "[", 
        RowBox[{"2", "Pi", " ", "k", " ", "x"}], "]"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"k", ",", "j"}], "}"}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"thetaesttprime", "[", 
    RowBox[{"t_", ",", "d_", ",", "j_"}], "]"}], ":=", 
   RowBox[{"2", 
    RowBox[{"Sum", "[", " ", 
     RowBox[{
