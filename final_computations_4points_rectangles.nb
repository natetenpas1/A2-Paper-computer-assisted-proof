   RowBox[{"thetaest", "[", 
    RowBox[{"x_", ",", "d_", ",", "j_"}], "]"}], ":=", 
   RowBox[{"1", "+", 
    RowBox[{"2", 
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"-", "d"}], " ", 
          RowBox[{"k", "^", "2"}]}], "]"}], 
        RowBox[{"Cos", "[", 
         RowBox[{"2", "Pi", " ", "k", " ", "x"}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", "j"}], "}"}]}], "]"}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"thetaestt", "[", 
    RowBox[{"t_", ",", "d_", ",", "j_"}], "]"}], ":=", 
   RowBox[{"1", "+", 
    RowBox[{"2", 
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"-", "d"}], " ", 
          RowBox[{"k", "^", "2"}]}], "]"}], 
        RowBox[{"Cos", "[", 
         RowBox[{"k", " ", 
          RowBox[{"ArcCos", "[", "t", "]"}]}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", "j"}], "}"}]}], "]"}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"thetaestprime", "[", 
